{
    "name": "mission10/craft-redirects",
    "description": "allows users to add redirects without regex",
    "type": "craft-plugin",
    "license": "MIT",
    "authors": [
      {
        "name": "Cameron Hay"
      }
    ],
    "support": {
      "email": "email@developer-website.tld",
      "issues": "https://github.com/developer/repo/issues?state=open",
      "source": "https://github.com/developer/repo",
      "docs": "https://github.com/developer/repo/blob/master/README.md"
    },
    "require": {
      "craftcms/cms": "^4.0.0-alpha",
      "ether/seo": "^4"
    },
    "autoload": {
      "psr-4": {
        "mission10\\redirects\\": "src/"
      }
    },
    "extra": {
      "name": "Redirects",
      "handle": "redirects",
      "class": "mission10\\redirects\\Redirects"
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    }
}
